{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "doctrine/dbal": "^3.6.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "itsgoingd/clockwork": "^v5.1.12",
        "laravel/framework": "^v10.1.3",
        "laravel/tinker": "^v2.8.1",
        "league/csv": "^9.8.0",
        "nyholm/psr7": "^1.5.1",
        "symfony/psr-http-message-bridge": "^v2.1.4",
        "tobyz/json-api-server": "^v0.2.0",
        "matthewbdaly/laravel-etag-middleware": "dev-l10-compatibility"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.21.0",
        "laravel/pint": "^v1.6.0",
        "laravel/sail": "^v1.21.0",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "v7.0.5",
        "phpunit/phpunit": "^10.0.11",
        "spatie/laravel-ignition": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-etag-middleware.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
